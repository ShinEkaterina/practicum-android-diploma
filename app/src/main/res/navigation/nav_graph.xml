<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/searchFragment2">

    <fragment
        android:id="@+id/searchFragment2"
        android:name="ru.practicum.android.diploma.ui.search.fragment.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment2_to_vacancyFragment3"
            app:destination="@id/vacancyFragment3"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
        <action
            android:id="@+id/action_searchFragment2_to_filterSettingsFragment"
            app:destination="@id/filterSettingsFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
    </fragment>
    <fragment
        android:id="@+id/teamFragment"
        android:name="ru.practicum.android.diploma.ui.team.TeamFragment"
        android:label="fragment_team"
        tools:layout="@layout/fragment_team" />
    <fragment
        android:id="@+id/vacancyFragment3"
        android:name="ru.practicum.android.diploma.ui.vacancy.VacancyFragment"
        android:label="fragment_vacancy"
        tools:layout="@layout/fragment_vacancy" >
        <action
            android:id="@+id/action_vacancyFragment3_to_similarVacancy"
            app:destination="@id/similarVacancy"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
        <action
            android:id="@+id/action_vacancyFragment3_to_detailEmployer"
            app:destination="@id/detailEmployer"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
    </fragment>
    <fragment
        android:id="@+id/favoritesFragment"
        android:name="ru.practicum.android.diploma.ui.favorites.FavoritesFragment"
        android:label="fragment_favorites"
        tools:layout="@layout/fragment_favorites" >
        <action
            android:id="@+id/action_favoritesFragment_to_vacancyFragment3"
            app:destination="@id/vacancyFragment3"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
    </fragment>
    <fragment
        android:id="@+id/filterSettingsFragment"
        android:name="ru.practicum.android.diploma.ui.filter.settings.FilterSettingsFragment"
        android:label="fragment_filter_settings"
        tools:layout="@layout/fragment_filter_settings" >
        <action
            android:id="@+id/action_filterSettingsFragment_to_industrySelectionFragment"
            app:destination="@id/industrySelectionFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
        <action
            android:id="@+id/action_filterSettingsFragment_to_choosingPlaceToJobFragment"
            app:destination="@id/choosingPlaceToJobFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
    </fragment>
    <fragment
        android:id="@+id/similarVacancy"
        android:name="ru.practicum.android.diploma.ui.similar.SimilarFragment"
        android:label="fragment_similar_vacancy"
        tools:layout="@layout/fragment_similar_vacancy" >
        <action
            android:id="@+id/action_similarVacancy_to_vacancyFragment3"
            app:destination="@id/vacancyFragment3"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
    </fragment>
    <fragment
        android:id="@+id/industrySelectionFragment"
        android:name="ru.practicum.android.diploma.ui.filter.industry.IndustrySelectionFragment"
        android:label="fragment_industry_selection"
        tools:layout="@layout/fragment_industry_selection" />
    <fragment
        android:id="@+id/choosingPlaceToJobFragment"
        android:name="ru.practicum.android.diploma.ui.filter.place.ChoosingPlaceToJobFragment"
        android:label="ChoosingPlaceToJobFragment" >
        <action
            android:id="@+id/action_choosingPlaceToJobFragment_to_regionSelectionFragment"
            app:destination="@id/regionSelectionFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
        <action
            android:id="@+id/action_choosingPlaceToJobFragment_to_countrySelectionFragment"
            app:destination="@id/countrySelectionFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
    </fragment>
    <fragment
        android:id="@+id/regionSelectionFragment"
        android:name="ru.practicum.android.diploma.ui.filter.region.RegionSelectionFragment"
        android:label="RegionSelectionFragment" />
    <fragment
        android:id="@+id/countrySelectionFragment"
        android:name="ru.practicum.android.diploma.ui.filter.country.CountrySelectionFragment"
        android:label="CountrySelectionFragment" />
    <fragment
        android:id="@+id/detailEmployer"
        android:name="ru.practicum.android.diploma.ui.employer.EmployerFragment"
        android:label="fragment_detail_employer"
        tools:layout="@layout/fragment_detail_employer" >
        <action
            android:id="@+id/action_detailEmployer_to_openVacanciesFragment"
            app:destination="@id/openVacanciesFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
    </fragment>
    <fragment
        android:id="@+id/openVacanciesFragment"
        android:name="ru.practicum.android.diploma.ui.similar.OpenVacanciesFragment"
        android:label="fragment_open_vacancies"
        tools:layout="@layout/fragment_open_vacancies" >
        <action
            android:id="@+id/action_openVacanciesFragment_to_vacancyFragment3"
            app:destination="@id/vacancyFragment3"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@anim/anim_in_left"
            app:popExitAnim="@anim/anim_out_right" />
    </fragment>
</navigation>
